df$ANY_CHEMO == "Chemo" &
df$DX_CHEMO_STARTED_DAYS > df$DX_RX_STARTED_DAYS,
1,
ifelse(
df$ANY_CHEMO == "Chemo" &
df$DX_CHEMO_STARTED_DAYS < df$DX_RX_STARTED_DAYS,
2,
3
)
)
)
