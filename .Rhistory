NCDBOS <- function(df){
# Make survival object
survObject <-
Surv(time = df$DX_LASTCONTACT_DEATH_MONTHS,
event = df$RECODED_STATUS)
# Plot overall survival curve
fit <-
survfit(survObject ~ 1, data = df, conf.type = "log-log")
